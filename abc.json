{
  "name": "refast-demo",
  "options": {
    "type": "uxcore",
    "template": "https://github.com/nowa-webpack/template-uxcore/archive/v4.zip",
    "npm": "tnpm",
    "vars": {
      "__LOCAL__": true
    },
    "buildvars": {
      "__LOCAL__": [false]
    },
    "pages": false,
    "externals": {
      "natty-fetch": "var nattyFetch",
      "react": "var React",
      "react-dom": "var ReactDOM",
      "react-router": "var ReactRouter",
      "uxcore": "var Uxcore",
      "lodash": "var _"
    },
      "injects": [
      "uxcore/assets/iconfont.css",
      "uxcore/assets/orange.css",
      "uxcore"
    ],
    "alias": {
      "components":"components",
      "db":"app/db.js",
      "variables":"app/variables.js"
    },
    "mockapi": {
      "/mock/user/query.json": "mock/user/query.js"
    }
  }
}
