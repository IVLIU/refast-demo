{
  "name": "refast-demo",
  "options": {
    "type": "uxcore",
    "template": "https://github.com/nowa-webpack/template-uxcore/archive/v4.zip",
    "lazyload": true,
    "loose": true,
    "npm": "tnpm",
    "vars": {
      "__LOCAL__": true
    },
    "buildvars": {
      "__LOCAL__": [
        false
      ]
    },
    "pages": false,
    "externals": {
      "react": "var React",
      "react-dom": "var ReactDOM",
      "react-router": "var ReactRouter"
    },
    "alias": {
      "components": "components",
      "db": "app/db.js",
      "variables": "app/variables.js"
    },
    "mockapi": {
      "/mock/user/query.json": "mock/user/query.js"
    }
  }
}
